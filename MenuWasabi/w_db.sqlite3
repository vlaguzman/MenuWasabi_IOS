DROP TABLE IF EXISTS "wsb_bebida";
CREATE TABLE "wsb_bebida" ("id_bebida" INTEGER PRIMARY KEY  NOT NULL ,"nombre" VARCHAR NOT NULL ,"id_tipoBebida" INTEGER, "id_categoria" INTEGER, "precio" INTEGER DEFAULT 0, FOREIGN KEY(id_tipoBebida) REFERENCES wsb_tipoBebida(id_tipoBebida), FOREIGN KEY(id_categoria) REFERENCES wsb_categoria(id_categoria));
DROP TABLE IF EXISTS "wsb_categoria";
CREATE TABLE "wsb_categoria" ("id_categoria" INTEGER PRIMARY KEY  NOT NULL  UNIQUE , "nombre" VARCHAR);
DROP TABLE IF EXISTS "wsb_plato";
CREATE TABLE "wsb_plato" ("id_plato" INTEGER PRIMARY KEY  NOT NULL ,"nombre" VARCHAR NOT NULL ,"descripcion" VARCHAR,"id_tipoPlato" INTEGER,"precio" INTEGER DEFAULT (0) ,"img_grande" VARCHAR,"img_pequena" VARCHAR,"img_principal" VARCHAR);
DROP TABLE IF EXISTS "wsb_tipoBebida";
CREATE TABLE "wsb_tipoBebida" ("id_tipoBebida" INTEGER PRIMARY KEY  NOT NULL ,"nombre" VARCHAR,"img_pequena" VARCHAR,"img_pedido" VARCHAR,"img_principal" VARCHAR);
DROP TABLE IF EXISTS "wsb_tipoPlato";
CREATE TABLE "wsb_tipoPlato" ("id_tipoPlato" INTEGER PRIMARY KEY  NOT NULL , "nombre" VARCHAR);
